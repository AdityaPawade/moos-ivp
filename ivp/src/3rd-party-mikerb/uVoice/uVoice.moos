// MOOS file

ServerHost = localhost
ServerPort = 9000

Community = base

ProcessConfig = ANTLER
{
	 MSBetweenLaunches = 200

	     Run = MOOSDB      @ NewConsole = false
		 Run = pMOOSBridge @ NewConsole = false ~ pMOOSBridge_base
}
								 

ProcessConfig = uVoice
{
	AppTick   = 4
	CommsTick = 4
}

ProcessConfig = pMOOSBridge_base
{
	AppTick   = 4
	CommsTick = 4

	SHARE = [zero_return]  -> nyak200 @ 192.168.0.200:9000 [uVoice_return]
	SHARE = [one_return]   -> nyak201 @ 192.168.0.201:9000 [uVoice_return]
	SHARE = [two_return]   -> nyak202 @ 192.168.0.202:9000 [uVoice_return]
	SHARE = [three_return] -> nyak203 @ 192.168.0.203:9000 [uVoice_return]
