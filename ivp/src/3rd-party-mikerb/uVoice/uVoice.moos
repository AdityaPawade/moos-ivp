	SHARE = [six_return]   -> nyak206 @ 192.168.0.206:9000 [uVoice_return]
	SHARE = [seven_return] -> nyak207 @ 192.168.0.207:9000 [uVoice_return]
	
	SHARE = [zero_follow]  -> nyak200 @ 192.168.0.200:9000 [uVoice_follow]
	SHARE = [one_follow]   -> nyak201 @ 192.168.0.201:9000 [uVoice_follow]
