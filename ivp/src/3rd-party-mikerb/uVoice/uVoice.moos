	SHARE = [two_formation]   -> nyak202 @ 192.168.0.202:9000 [uVoice_formation]
	SHARE = [three_formation] -> nyak203 @ 192.168.0.203:9000 [uVoice_formation]
	SHARE = [four_formation]  -> nyak204 @ 192.168.0.204:9000 [uVoice_formation]
