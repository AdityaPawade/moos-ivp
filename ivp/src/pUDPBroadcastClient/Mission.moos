ServerHost = localhost
ServerPort = 9000


ProcessConfig = pUDPBroadcastClient
{
AppTick = 1
CommsTick = 1

// You can specify the address to recieve from here but in general you wouldn't want to. 
// Just use *:port_number
//Example, to recieve on port number 9097   *:9097

// Bluefin Dashboard UDP Broadcast default port: 4477
BroadcastAddress = *:9097 


}
