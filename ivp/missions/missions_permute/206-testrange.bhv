Behavior = BHV_OpRegion
{	
   name = bhv_op_region
   pwt  = 100

   polygon = label,SafeOpBox:-380,-160:-220,-630:490,-380:400,60:90,20
   trigger_entry_time = 2
   trigger_exit_time  = 2
}
